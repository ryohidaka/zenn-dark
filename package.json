{
  "name": "zenn-dark",
  "description": "Zennのサイトをダークモードに変更するブラウザ拡張機能",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "format": "npx prettier './' --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohidaka/zenn-dark.git"
  },
  "author": "ryohidaka",
  "bugs": {
    "url": "https://github.com/ryohidaka/zenn-dark/issues"
  },
  "homepage": "https://github.com/ryohidaka/zenn-dark#readme",
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "typescript": "^5.7.2",
    "wxt": "^0.19.19"
  }
}
